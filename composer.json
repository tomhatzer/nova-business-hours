{
    "name": "tomhatzer/nova-business-hours",
    "description": "Business hours field for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Tomhatzer\\NovaBusinessHours\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tomhatzer\\NovaBusinessHours\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
